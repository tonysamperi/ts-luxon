{
    "name": "ts-luxon",
    "version": "3.5.2",
    "license": "MIT",
    "description": "Typescript version of the \"Immutable date wrapper\"",
    "author": "Tony Samperi",
    "keywords": [
        "date",
        "immutable",
        "typescript"
    ],
    "repository": "https://github.com/tonysamperi/ts-luxon",
    "main": "dist/ts-luxon.umd.js",
    "module": "dist/ts-luxon.es6.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist/**",
        "README.md"
    ],
    "engines": {
        "node": ">=13"
    },
    "scripts": {
        "build:watch": "rollup -c rollup.config.js -w",
        "build": "rollup -c rollup.config.js",
        "test": "jest --coverage",
        "lint": "tslint --project tsconfig.json",
        "docs": "esdoc -c docs/index.js",
        "site": "cp -r site dist",
        "benchmark": "babel-node benchmarks/index.js",
        "codecov": "codecov",
        "check-doc-coverage": "grep '\"coverage\": \"100%\"' dist/docs/coverage.json",
        "publish": "npm publish --tag latest-v3"
    },
    "devDependencies": {
        "@rollup/plugin-replace": "^5.0.2",
        "@types/benchmark": "^2.1.0",
        "@types/jest": "^27.4.0",
        "benchmark": "^2.0.0",
        "codecov": "^3.0.0",
        "jest": "^27.4.7",
        "rollup": "~2.33.3",
        "rollup-plugin-cleaner": "^1.0.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-sourcemaps": "~0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "~0.29.0",
        "rollup-plugin-uglify": "^6.0.4",
        "ts-jest": "^27.1.3",
        "tslib": "^2.0.0",
        "tslint": "^6.1.3",
        "tslint-consistent-codestyle": "^1.16.0",
        "typescript": "~3.9.7"
    },
    "sideEffects": false
}
